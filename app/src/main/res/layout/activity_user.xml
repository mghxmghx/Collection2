<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.viewpager.widget.ViewPager
            android:id="@+id/pagerName"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/black_gray"
        android:gravity="left|center"
        android:orientation="horizontal"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/home_lay"
            android:layout_marginStart="8dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/home_ic"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:visibility="gone"
                app:srcCompat="@drawable/home_ic"
                app:tint="@color/ic_bg"
                tools:ignore="VectorDrawableCompat" />

            <LinearLayout
                android:id="@+id/home_lay_in"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:background="@drawable/indicator_ic"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingRight="5dp">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_home_line"
                    app:tint="@color/ic_bg"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/MessagesAll3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Home"
                    android:textColor="@color/ic_bg"
                    android:textSize="16dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/drink_lay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/drink_ic"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:visibility="visible"
                app:srcCompat="@drawable/ic_beer_outline"
                app:tint="@color/ic_bg"
                tools:ignore="VectorDrawableCompat" />

            <LinearLayout
                android:id="@+id/drink_lay_in"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:visibility="gone"
                android:background="@drawable/indicator_ic"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingRight="5dp">

                <ImageView
                    android:id="@+id/imageView4d"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_beer_outline"
                    app:tint="@color/ic_bg"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/MessagesAldl3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Drink"
                    android:textColor="@color/ic_bg"
                    android:textSize="16dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/user_lay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/user_ic"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:visibility="visible"
                app:srcCompat="@drawable/ic_user_line"
                app:tint="@color/ic_bg"
                tools:ignore="VectorDrawableCompat" />

            <LinearLayout
                android:id="@+id/user_lay_in"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:visibility="gone"
                android:background="@drawable/indicator_ic"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingRight="5dp">

                <ImageView
                    android:id="@+id/imageView44"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_user_line"
                    app:tint="@color/ic_bg"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/MessagesAll34"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Personal"
                    android:textColor="@color/ic_bg"
                    android:textSize="16dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/phone_lay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/phone_ic"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:visibility="visible"
                app:srcCompat="@drawable/ic_statistics"
                app:tint="@color/ic_bg"
                tools:ignore="VectorDrawableCompat" />

            <LinearLayout
                android:id="@+id/phone_lay_in"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:visibility="gone"
                android:background="@drawable/indicator_ic"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="5dp"
                android:paddingRight="5dp">

                <ImageView
                    android:id="@+id/imageView34"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_statistics"
                    app:tint="@color/ic_bg"
                    tools:ignore="VectorDrawableCompat" />

                <TextView
                    android:id="@+id/MessagesAll32"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Stats."
                    android:textColor="@color/ic_bg"
                    android:textSize="16dp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>